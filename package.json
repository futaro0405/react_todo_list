{
  "name": "react_todo_list",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://futaro0405.github.io/react_todo_list/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepare": "husky install",
    "preview": "vite preview",
    "lint": "eslint src/ --ext js,jsx",
    "lint:format": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|yml|css|scss)\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "gh-pages": "6.1.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "vite": "5.0.8"
  },
  "volta": {
    "node": "20.10.0"
  }
